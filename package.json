    "astro-expressive-code": "^0.34.1",
    "dotenv": "^16.0.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^2.1.1",
    "rehype-infer-reading-time-meta": "^1.1.0",
